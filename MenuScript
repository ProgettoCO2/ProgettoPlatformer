
public void quitToAnotherScene(string loadedScene)
{
  if (loadedScene.equals("NULL"){
    Application.Quit(); 
  }
  else {
    LoadScene(loadedScene);
  }
}

//usare solo un canvas con argomento? SI, il parametro pu√≤ essere passato da inspector dice il web
public void loadedCanvas(Canvas canvasSelected){
  canvasSelected.setActive(true);
}

